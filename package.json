{
  "name": "@example/basics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "astro:build": "astro build",
    "astro:dev": "astro dev",
    "cloudinary:create-transformations": "ts-node ./content/scripts/create-cloudinary-transformations.ts",
    "content:cache-static-map-urls": "ts-node ./content/scripts/cache-static-map-urls.ts",
    "content:generate-cache": "ts-node ./content/scripts/generate-content-cache.ts",
    "content:watch": "nodemon ./content/scripts/generate-content-cache.ts",
    "check": "astro check",
    "dev": "npm-run-all -p content:watch astro:dev",
    "start": "astro dev",
    "build": "npm-run-all -s content:generate-cache check astro:build",
    "preview": "astro preview",
    "astro": "astro",
    "stackbit-cloud:dev": "npm-run-all -p content:watch stackbit-cloud:astro:dev",
    "stackbit-cloud:astro:dev": "./node_modules/.bin/astro dev -- --port {PORT}"
  },
  "dependencies": {
    "@astrojs/tailwind": "^3.0.0",
    "@swc/core": "^1.3.68",
    "astro": "^2.0.7",
    "cloudinary": "^1.34.0",
    "mapbox-gl": "^2.13.0",
    "markdown-it": "^13.0.1",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.0.24",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@stackbit/cms-git": "^0.3.20",
    "@stackbit/types": "^0.10.0",
    "@types/gtag.js": "^0.0.12",
    "@types/mapbox-gl": "^2.7.10",
    "@types/node": "^20.4.3",
    "axios": "^1.3.3",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.8.0",
    "ts-node": "^10.9.1"
  },
  "nodemonConfig": {
    "watch": [
      "./content/**/*"
    ],
    "ext": "ts,json,md",
    "ignore": [
      "./content/generated/**/*.json"
    ]
  }
}
